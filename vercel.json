{
  "version": 2,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.fadhil.qzz.io"
        }
      ],
      "destination": "https://fadhil.qzz.io/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/favicon.png",
      "destination": "/favicon.ico"
    }
  ]
}
